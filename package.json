{
  "name": "ts-pipeline",
  "version": "1.0.0",
  "description": "Generic request/response pipeline for TypeScript",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "mocha dist/tests",
    "build": "tsc && npm run definition",
    "definition": "tsc ts-pipeline.ts -t es5 --outFile ts-pipeline.js -m system -d --noEmitOnError && rm ts-pipeline.js && sed -i '' -e 's/\"src\\//\"__ts-pipeline\\/src\\//g' ts-pipeline.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilniklas/ts-pipeline.git"
  },
  "keywords": [
    "pipeline",
    "request",
    "response",
    "typescript"
  ],
  "author": "Emil Persson <emil.n.persson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emilniklas/ts-pipeline/issues"
  },
  "homepage": "https://github.com/emilniklas/ts-pipeline#readme",
  "devDependencies": {
    "assert": "^1.3.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "ts-optional": "^1.0.0"
  }
}
